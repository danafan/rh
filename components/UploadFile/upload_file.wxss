/* components/UploadFile/upload_file.wxss */
.upload_box {
  border: 1rpx solid #e6e6e6;
  width: 210rpx;
  height: 210rpx;
  background: #f6f6f6;
  border-radius: 5rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.upload_icon{
  margin-bottom: 8rpx;
  width: 55rpx;
  height: 45rpx;
}
.upload_text{
  font-size: 26rpx;
  color: #8a8a8a;
}